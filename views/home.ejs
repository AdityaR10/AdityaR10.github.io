
<%- include("partials/header"); -%>
  <div class="default-home">
    <h1>Home</h1>
    <p> <%= startingContent %> </p>
  </div>
<div class="post-render-container">
  <%  posts.forEach(function(post){ %>
  <div class=" default-home render-post">
    <h1><%=post.title%></h1>
    <p>
    <%=post.content.substring(0, 100) + " ..."%>
    <a href="/posts/<%=post.title%>">Read More</a>
    </p>
  </div>

    <% }) %>
  </div>

<%- include("partials/footer"); -%>
